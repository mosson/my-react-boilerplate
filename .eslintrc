{
  "parser": "babel-eslint",
  "plugins": [
    "jasmine",
    "react",
    "flowtype"
  ],
  "globals": {
    "$": true,
    "__DEV__": true
  },
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true,
    "es6": true
  },
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "rules": {
    "complexity": [1, 5],
    "key-spacing": 0,
    "new-cap": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "no-trailing-spaces": 0,
    "no-unused-vars": 0,
    "handle-callback-err": 0,
    "camelcase": 0,
    "no-unused-expressions": 0,
    "curly": 0,
    "quotes": [0, "double"],
    "react/jsx-uses-vars": 1,
    "jasmine/no-suite-dupes": "branch",
    "jasmine/no-spec-dupes": 0
  }
}
